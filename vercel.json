{
  "version": 2,
  "routes": [
    { "src": "/rooms/(.*)", "dest": "/backend/rooms/$1" },
    { "src": "/users/(.*)", "dest": "/backend/users/$1" },
    { "src": "/spotify/(.*)", "dest": "/backend/spotify/$1" },
    { "src": "/auth/spotify/(.*)", "dest": "/backend/auth/spotify/$1" },
